{
  "name": "acting-portfolio",
  "version": "1.0.0",
  "description": "An acting site temp",
  "main": "server.js",
  "dependencies": {
    "animate.css": "^4.1.1",
    "bootstrap": "^4.5.3",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "react-bootstrap": "^1.4.0",
    "react-bootstrap-typeahead": "^5.1.2",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^6.1.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.4"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js && node scripts/seedMenu.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lambaa94/Acting-Portfolio.git"
  },
  "author": "James Lamb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lambaa94/Acting-Portfolio/issues"
  },
  "homepage": "https://github.com/Lambaa94/Acting-Portfolio#readme"
}
